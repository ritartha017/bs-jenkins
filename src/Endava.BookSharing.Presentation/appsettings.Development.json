{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TokenSettings": {
    "Issuer": "http://booksharing.local",
    "Audience": "http://booksharing.local",
    "Secret": "veryVerySuperSecretKey",
    "ExpirationInMinutes": 30
  },
  "PasswordResetTokenSettings": {
    "LifetimeInMinutes": 5
  },
  "ConnectionStrings": {
    "Postgresql": "Server=db;Database=booksharingdb;Port=5432;Username=postgres;Password=postgres"
  },
  "BookCoverOptions": {
    "Url": "http://localhost:5027/book/cover?book_id={0}",
    "NoCoverUrl": "https://upload.wikimedia.org/wikipedia/commons/b/b9/No_Cover.jpg"
  },
  "CorsOptions": {
    "AccessControlAllowOrigin": "http://localhost:3000"
  }
}
